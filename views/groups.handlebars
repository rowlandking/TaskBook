<body>


<nav class="navbar navbar-default navbar-static-top" role="navigation">
  <div class="logo">  <img class ="logotext" src="/images/taskbook.png"></img> </div>
  <div class="container" style="text-align: center">
    <!-- Old Button<button type="button" class="btn btn-default navbar-btn fleft" ><span class="glyphicon glyphicon-th-large"></span> </button>-->
    <div class="btn-group fleft buttoncenter">
      <div class="btn-group">
        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
          <span class="glyphicon glyphicon-th-large"></span> 
        </button>
        <ul class="dropdown-menu">
          {{#each groups}}
          <li><a href="/groups/{{name}}">{{name}}</a></li>
          {{/each}}
        </ul>
      </div>
    </div>
    <button type="button" class="btn btn-default navbar-btn fright" ><span class="glyphicon glyphicon-cog"></span> </button>
    <button type="button" class="btn btn-default navbar-btn fright" ><span class="glyphicon glyphicon-bullhorn"></span> </button>

  </div>
</nav>


<h2 class="groupTitleText">{{projectName}}</h2>
<div class="groupTitle">
  <button type="button" class="btn btn-default navbar-btn fleft pin" ><span class="glyphicon glyphicon-pushpin"></span> </button>
  <button type="button" class="btn btn-default navbar-btn fleft pin" ><span class="glyphicon glyphicon-sort"></span> </button>
  <button type="button" class="btn btn-default navbar-btn fright pin" ><span class="glyphicon glyphicon-filter"></span> </button>
  <button type="button" class="btn btn-default navbar-btn fright pin" ><span class="glyphicon glyphicon-pencil"></span> </button>
</div>


